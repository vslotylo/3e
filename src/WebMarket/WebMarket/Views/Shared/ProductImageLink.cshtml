@model WebMarket.DAL.Entities.Product

<a href=@Url.Action("details", Model.Controller, new { id = Model.Id }) >
    <table width="100%" height="100%" align="center" valign="center">
    <tr>
        <td>
            <img class="details-image2" src="@string.Format("/Content/tiles/{0}", @Model.GetListPhoto())" onerror="this.src='/Content/tiles/noimage.png'" alt="@Model.Name" title="@Model.Name"/>
        </td>
    </tr>
</table>
</a>

